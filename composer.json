{
    "name": "denmasyarikin/laravel-encrypt-response",
    "version": "1.0.7",
    "description": "Encrypt response laravel appliction",
    "license": "MIT",
    "keywords": [
        "response",
        "encryption",
        "laravel"
    ],
    "homepage": "https://github.com/denmasyarikin/laravel-encrypt-response",
    "authors": [
        {
            "name": "Denma Syarikin",
            "email": "denmasyarikin@gmail.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "brainfoolong/cryptojs-aes-php": "^2.1",
        "illuminate/http": "^8.0"
    },
    "autoload": {
        "psr-4": {
          "Denmasyarikin\\EncryptResponse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Denmasyarikin\\EncryptResponse\\Providers\\LaravelServiceProvider"
            ]
        }
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix . --rules=@Symfony"
        ]
    }
}
