{
    "name": "denmasyarikin/laravel-encrypt-response",
    "version": "3.0.0",
    "description": "Encrypt response laravel appliction",
    "license": "MIT",
    "keywords": [
        "response",
        "encryption",
        "laravel"
    ],
    "homepage": "https://github.com/denmasyarikin/laravel-encrypt-response",
    "authors": [
        {
            "name": "Denma Syarikin",
            "email": "denmasyarikin@gmail.com"
        }
    ],
    "require": {
        "brainfoolong/cryptojs-aes-php": "^2.2",
        "illuminate/http": "^10|^11",
        "illuminate/contracts": "^10|^11",
        "illuminate/support": "^10|^11"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "laravel/framework": "^10|^11"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
          "Denmasyarikin\\EncryptResponse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Denmasyarikin\\EncryptResponse\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "fix":  "php-cs-fixer fix . --rules=@Symfony"
    }
}
